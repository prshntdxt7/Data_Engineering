{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-deloads"
		},
		"dbdeloads_service_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'dbdeloads_service'"
		},
		"deloadsadls_service_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'deloadsadls_service'"
		},
		"sql_db_service_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'sql_db_service'"
		},
		"deloadsadls_service_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://deloads.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf-deloads_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"dbdeloads_service_connectionString": {
						"value": "[parameters('dbdeloads_service_connectionString')]"
					},
					"deloadsadls_service_accountKey": {
						"value": "[parameters('deloadsadls_service_accountKey')]"
					},
					"sql_db_service_connectionString": {
						"value": "[parameters('sql_db_service_connectionString')]"
					},
					"deloadsadls_service_properties_typeProperties_url": {
						"value": "[parameters('deloadsadls_service_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}