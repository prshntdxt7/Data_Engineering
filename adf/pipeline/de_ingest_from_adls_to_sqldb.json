{
	"name": "de_ingest_from_adls_to_sqldb",
	"properties": {
		"activities": [
			{
				"name": "Copy_ooh",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [
					{
						"name": "Source",
						"value": "exports//address_table_data"
					},
					{
						"name": "Destination",
						"value": "dbo.address_table_data_adf_de_loads"
					}
				],
				"typeProperties": {
					"source": {
						"type": "JsonSource",
						"storeSettings": {
							"type": "AzureBlobFSReadSettings",
							"recursive": true,
							"enablePartitionDiscovery": false
						},
						"formatSettings": {
							"type": "JsonReadSettings"
						}
					},
					"sink": {
						"type": "AzureSqlSink",
						"tableOption": "autoCreate"
					},
					"enableStaging": false,
					"validateDataConsistency": false,
					"logSettings": {
						"enableCopyActivityLog": true,
						"copyActivityLogSettings": {
							"logLevel": "Info",
							"enableReliableLogging": false
						},
						"logLocationSettings": {
							"linkedServiceName": {
								"referenceName": "deloadsadls_service",
								"type": "LinkedServiceReference"
							},
							"path": "logs"
						}
					}
				},
				"inputs": [
					{
						"referenceName": "SourceDataset_ooh",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "DestinationDataset_ooh",
						"type": "DatasetReference"
					}
				]
			}
		],
		"annotations": []
	}
}